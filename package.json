{
    "name": "streaming-scraper",
    "version": "1.0.2",
    "main": "./src/index.js",
    "exports": {
        ".": "./src/index.js"
    },
    "type": "module",
    "scripts": {
        "dev": "bun --watch ./src --debug",
        "main": "bun --watch ./src/index.js",
        "deploy": "bun --watch ./src/index.js",
        "start": "bun --watch ./src/index.js",
        "build:win": "bun build src/index.js --compile --outfile ./dist/scraper --target=bun-windows-x64 --define process.argv=\"['--no-log']\"",
        "build:mac": "bun build src/index.js --compile --outfile dist/streaming-scraper --target=bun-darwin-x64 --define process.argv=\"['--no-log']\"",
        "build:linux": "bun build src/index.js --compile --outfile dist/streaming-scraper --target=bun-linux-x64 --define process.argv=\"['--no-log']\""
    },
    "dependencies": {
        "axios": "^1.13.2",
        "base-64": "^1.0.0",
        "cheerio": "^1.1.2",
        "cloudflare-scraper": "^2.0.0",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.3",
        "node-cache": "^5.1.2",
        "node-fetch": "^3.3.2",
        "prettier": "^3.7.3",
        "unpacker": "^1.0.1",
        "wyzie-lib": "^2.2.5"
    },
    "devDependencies": {
        "pkgroll": "^2.21.4"
    }
}
